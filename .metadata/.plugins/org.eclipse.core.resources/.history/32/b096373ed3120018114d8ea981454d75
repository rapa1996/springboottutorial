package com.parameter.greeting;

public class Greeting {
	
	private String name;
	private Integer id;
	
	public Greeting(Integer id, String name) {
		super();
		this.name = name;
		this.id = id;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
