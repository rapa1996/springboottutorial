package com.parameter.greeting;

public class Greeting {
	
	private String name;
	private long id;
	
	public Greeting(long id, String name) {
		super();
		this.name = name;
		this.id = id;
	}
	
	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
